{
  "header_templates": {
    "minimal": {
      "description": "Clean minimal header",
      "template": "\"use client\"\n\nimport React, { useState } from 'react'\nimport { ComponentInfo } from '@/lib/cms-types'\nimport { Menu, X } from 'lucide-react'\n\nexport const metadata: ComponentInfo = {\n  type: 'Header',\n  name: 'Header',\n  description: 'Site navigation and branding header',\n  category: 'layout',\n  icon: 'Navigation',\n}\n\nexport default function Header() {\n  const [isMenuOpen, setIsMenuOpen] = useState(false)\n\n  return (\n    <header className=\"w-full bg-white border-b border-theme-gray-100 sticky top-0 z-50\">\n      <div className=\"theme-container\">\n        <div className=\"flex items-center justify-between h-14\">\n          <div className=\"flex-shrink-0\">\n            <a href=\"/\" className=\"flex items-center\">\n              <span className=\"text-lg font-semibold text-theme-gray-900\">{{LOGO_TEXT}}</span>\n            </a>\n          </div>\n          <nav className=\"hidden md:flex items-center space-x-6\">\n            <a href=\"/\" className=\"text-sm text-theme-gray-600 hover:text-theme-primary-600\">Home</a>\n            <a href=\"/about\" className=\"text-sm text-theme-gray-600 hover:text-theme-primary-600\">About</a>\n            <a href=\"/services\" className=\"text-sm text-theme-gray-600 hover:text-theme-primary-600\">Services</a>\n            <a href=\"/contact\" className=\"text-sm text-theme-gray-600 hover:text-theme-primary-600\">Contact</a>\n          </nav>\n          <div className=\"hidden md:flex\">\n            <button className=\"px-4 py-2 text-sm bg-theme-primary-500 text-white rounded-md hover:bg-theme-primary-600\">\n              {{CTA_BUTTON}}\n            </button>\n          </div>\n          <div className=\"md:hidden\">\n            <button onClick={() => setIsMenuOpen(!isMenuOpen)} className=\"p-2\">\n              {isMenuOpen ? <X className=\"h-5 w-5\" /> : <Menu className=\"h-5 w-5\" />}\n            </button>\n          </div>\n        </div>\n        {isMenuOpen && (\n          <div className=\"md:hidden pb-4\">\n            <div className=\"space-y-2\">\n              <a href=\"/\" className=\"block py-2 text-theme-gray-600\">Home</a>\n              <a href=\"/about\" className=\"block py-2 text-theme-gray-600\">About</a>\n              <a href=\"/services\" className=\"block py-2 text-theme-gray-600\">Services</a>\n              <a href=\"/contact\" className=\"block py-2 text-theme-gray-600\">Contact</a>\n              <button className=\"w-full mt-3 px-4 py-2 bg-theme-primary-500 text-white rounded-md\">\n                {{CTA_BUTTON}}\n              </button>\n            </div>\n          </div>\n        )}\n      </div>\n    </header>\n  )\n}"
    },
    "modern": {
      "description": "Modern header with gradient background",
      "template": "\"use client\"\n\nimport React, { useState } from 'react'\nimport { ComponentInfo } from '@/lib/cms-types'\nimport { Menu, X, Zap } from 'lucide-react'\n\nexport const metadata: ComponentInfo = {\n  type: 'Header',\n  name: 'Header',\n  description: 'Site navigation and branding header',\n  category: 'layout',\n  icon: 'Navigation',\n}\n\nexport default function Header() {\n  const [isMenuOpen, setIsMenuOpen] = useState(false)\n\n  return (\n    <header className=\"w-full bg-gradient-to-r from-theme-primary-900 to-theme-primary-700 sticky top-0 z-50 shadow-lg\">\n      <div className=\"theme-container\">\n        <div className=\"flex items-center justify-between h-16\">\n          <div className=\"flex-shrink-0\">\n            <a href=\"/\" className=\"flex items-center\">\n              <div className=\"h-8 w-8 bg-white rounded-lg flex items-center justify-center mr-3\">\n                <Zap className=\"h-5 w-5 text-theme-primary-600\" />\n              </div>\n              <span className=\"text-xl font-bold text-white\">{{LOGO_TEXT}}</span>\n            </a>\n          </div>\n          <nav className=\"hidden md:flex items-center space-x-8\">\n            <a href=\"/\" className=\"text-white/90 hover:text-white px-3 py-2 text-sm font-medium\">Home</a>\n            <a href=\"/about\" className=\"text-white/90 hover:text-white px-3 py-2 text-sm font-medium\">About</a>\n            <a href=\"/services\" className=\"text-white/90 hover:text-white px-3 py-2 text-sm font-medium\">Services</a>\n            <a href=\"/contact\" className=\"text-white/90 hover:text-white px-3 py-2 text-sm font-medium\">Contact</a>\n          </nav>\n          <div className=\"hidden md:flex\">\n            <button className=\"px-6 py-3 bg-white text-theme-primary-700 rounded-lg font-medium hover:bg-theme-gray-100 transition-all shadow-md\">\n              {{CTA_BUTTON}}\n            </button>\n          </div>\n          <div className=\"md:hidden\">\n            <button onClick={() => setIsMenuOpen(!isMenuOpen)} className=\"p-2 text-white\">\n              {isMenuOpen ? <X className=\"h-6 w-6\" /> : <Menu className=\"h-6 w-6\" />}\n            </button>\n          </div>\n        </div>\n        {isMenuOpen && (\n          <div className=\"md:hidden pb-4\">\n            <div className=\"space-y-2\">\n              <a href=\"/\" className=\"block py-3 px-4 text-white/90 hover:text-white hover:bg-white/10 rounded\">Home</a>\n              <a href=\"/about\" className=\"block py-3 px-4 text-white/90 hover:text-white hover:bg-white/10 rounded\">About</a>\n              <a href=\"/services\" className=\"block py-3 px-4 text-white/90 hover:text-white hover:bg-white/10 rounded\">Services</a>\n              <a href=\"/contact\" className=\"block py-3 px-4 text-white/90 hover:text-white hover:bg-white/10 rounded\">Contact</a>\n              <button className=\"w-full mt-4 px-6 py-3 bg-white text-theme-primary-700 rounded-lg font-medium\">\n                {{CTA_BUTTON}}\n              </button>\n            </div>\n          </div>\n        )}\n      </div>\n    </header>\n  )\n}"
    },
    "centered": {
      "description": "Centered header layout",
      "template": "\"use client\"\n\nimport React from 'react'\nimport { ComponentInfo } from '@/lib/cms-types'\n\nexport const metadata: ComponentInfo = {\n  type: 'Header',\n  name: 'Header',\n  description: 'Site navigation and branding header',\n  category: 'layout',\n  icon: 'Navigation',\n}\n\nexport default function Header() {\n  return (\n    <header className=\"w-full bg-white border-b border-theme-gray-200 sticky top-0 z-50\">\n      <div className=\"theme-container\">\n        <div className=\"py-6 text-center\">\n          <div className=\"mb-4\">\n            <a href=\"/\" className=\"inline-block\">\n              <span className=\"text-2xl font-bold text-theme-gray-900\">{{LOGO_TEXT}}</span>\n            </a>\n          </div>\n          <nav className=\"flex items-center justify-center space-x-8 mb-4\">\n            <a href=\"/\" className=\"text-theme-gray-700 hover:text-theme-primary-600 px-3 py-2 text-sm font-medium\">Home</a>\n            <a href=\"/about\" className=\"text-theme-gray-700 hover:text-theme-primary-600 px-3 py-2 text-sm font-medium\">About</a>\n            <a href=\"/services\" className=\"text-theme-gray-700 hover:text-theme-primary-600 px-3 py-2 text-sm font-medium\">Services</a>\n            <a href=\"/contact\" className=\"text-theme-gray-700 hover:text-theme-primary-600 px-3 py-2 text-sm font-medium\">Contact</a>\n          </nav>\n          <div>\n            <button className=\"px-6 py-3 bg-theme-primary-500 text-white rounded-lg font-medium hover:bg-theme-primary-600 transition-all\">\n              {{CTA_BUTTON}}\n            </button>\n          </div>\n        </div>\n      </div>\n    </header>\n  )\n}"
    }
  },
  "hero_templates": {
    "split": {
      "description": "Split-screen hero layout",
      "template": "import React from 'react'\nimport { ComponentInfo } from '@/lib/cms-types'\nimport { ArrowRight, Play } from 'lucide-react'\n\nexport const metadata: ComponentInfo = {\n  type: 'Hero',\n  name: 'Hero Section',\n  description: 'Main landing section with headline and CTA',\n  category: 'content-blocks',\n  icon: 'Zap',\n}\n\nexport default function Hero() {\n  return (\n    <section className=\"w-full min-h-screen bg-theme-gray-50\">\n      <div className=\"theme-container h-screen\">\n        <div className=\"grid lg:grid-cols-2 h-full\">\n          <div className=\"flex flex-col justify-center space-y-8 py-12\">\n            <div className=\"space-y-6\">\n              <h1 className=\"text-4xl lg:text-6xl font-bold text-theme-gray-900 leading-tight\">\n                {{HEADLINE}}\n              </h1>\n              <p className=\"text-xl text-theme-gray-600 leading-relaxed max-w-lg\">\n                {{SUBTEXT}}\n              </p>\n            </div>\n            <div className=\"flex flex-col sm:flex-row gap-4\">\n              <button className=\"inline-flex items-center justify-center px-8 py-4 bg-theme-primary-500 text-white rounded-lg font-medium hover:bg-theme-primary-600 transition-all\">\n                {{CTA_PRIMARY}}\n                <ArrowRight className=\"ml-2 h-5 w-5\" />\n              </button>\n              <button className=\"inline-flex items-center justify-center px-8 py-4 border-2 border-theme-gray-200 text-theme-gray-900 rounded-lg font-medium hover:bg-theme-gray-50 transition-all\">\n                <Play className=\"mr-2 h-5 w-5\" />\n                {{CTA_SECONDARY}}\n              </button>\n            </div>\n          </div>\n          <div className=\"hidden lg:flex items-center justify-center bg-gradient-to-br from-theme-primary-100 to-theme-primary-200\">\n            <div className=\"w-96 h-96 bg-theme-primary-500 rounded-full opacity-20\"></div>\n          </div>\n        </div>\n      </div>\n    </section>\n  )\n}"
    },
    "fullscreen": {
      "description": "Full-screen hero with centered content",
      "template": "import React from 'react'\nimport { ComponentInfo } from '@/lib/cms-types'\nimport { ArrowRight, ChevronDown } from 'lucide-react'\n\nexport const metadata: ComponentInfo = {\n  type: 'Hero',\n  name: 'Hero Section',\n  description: 'Main landing section with headline and CTA',\n  category: 'content-blocks',\n  icon: 'Zap',\n}\n\nexport default function Hero() {\n  return (\n    <section className=\"relative w-full h-screen bg-gradient-to-br from-theme-gray-900 via-theme-primary-900 to-theme-gray-900 overflow-hidden\">\n      <div className=\"absolute inset-0 bg-black/20\"></div>\n      <div className=\"relative theme-container h-full flex items-center justify-center\">\n        <div className=\"text-center space-y-8 max-w-4xl\">\n          <h1 className=\"text-5xl lg:text-7xl font-bold text-white leading-tight\">\n            {{HEADLINE}}\n          </h1>\n          <p className=\"text-xl lg:text-2xl text-white/90 leading-relaxed max-w-3xl mx-auto\">\n            {{SUBTEXT}}\n          </p>\n          <div className=\"flex flex-col sm:flex-row gap-6 justify-center\">\n            <button className=\"inline-flex items-center justify-center px-10 py-5 bg-white text-theme-gray-900 rounded-lg font-semibold hover:bg-theme-gray-100 transition-all text-lg\">\n              {{CTA_PRIMARY}}\n              <ArrowRight className=\"ml-3 h-6 w-6\" />\n            </button>\n            <button className=\"inline-flex items-center justify-center px-10 py-5 border-2 border-white text-white rounded-lg font-semibold hover:bg-white/10 transition-all text-lg\">\n              {{CTA_SECONDARY}}\n            </button>\n          </div>\n        </div>\n      </div>\n      <div className=\"absolute bottom-8 left-1/2 transform -translate-x-1/2 text-white/60\">\n        <ChevronDown className=\"h-8 w-8 animate-bounce\" />\n      </div>\n    </section>\n  )\n}"
    },
    "modern": {
      "description": "Modern hero with cards and gradients",
      "template": "import React from 'react'\nimport { ComponentInfo } from '@/lib/cms-types'\nimport { ArrowRight, Star, Users, Zap } from 'lucide-react'\n\nexport const metadata: ComponentInfo = {\n  type: 'Hero',\n  name: 'Hero Section',\n  description: 'Main landing section with headline and CTA',\n  category: 'content-blocks',\n  icon: 'Zap',\n}\n\nexport default function Hero() {\n  return (\n    <section className=\"w-full py-20 lg:py-32 bg-gradient-to-br from-theme-gray-50 via-white to-theme-primary-50\">\n      <div className=\"theme-container\">\n        <div className=\"text-center space-y-12\">\n          <div className=\"space-y-6\">\n            <div className=\"inline-flex items-center bg-theme-primary-100 text-theme-primary-700 px-4 py-2 rounded-full text-sm font-medium\">\n              <Star className=\"h-4 w-4 mr-2\" />\n              Trusted by 10,000+ customers\n            </div>\n            <h1 className=\"text-4xl lg:text-6xl font-bold text-theme-gray-900 leading-tight max-w-4xl mx-auto\">\n              {{HEADLINE}}\n            </h1>\n            <p className=\"text-xl text-theme-gray-600 leading-relaxed max-w-2xl mx-auto\">\n              {{SUBTEXT}}\n            </p>\n          </div>\n          <div className=\"flex flex-col sm:flex-row gap-6 justify-center\">\n            <button className=\"inline-flex items-center justify-center px-8 py-4 bg-theme-primary-500 text-white rounded-xl font-semibold hover:bg-theme-primary-600 transition-all shadow-lg hover:shadow-xl\">\n              {{CTA_PRIMARY}}\n              <ArrowRight className=\"ml-2 h-5 w-5\" />\n            </button>\n            <button className=\"inline-flex items-center justify-center px-8 py-4 bg-white border-2 border-theme-gray-200 text-theme-gray-900 rounded-xl font-semibold hover:bg-theme-gray-50 transition-all\">\n              {{CTA_SECONDARY}}\n            </button>\n          </div>\n          <div className=\"grid grid-cols-1 sm:grid-cols-3 gap-6 mt-16 max-w-4xl mx-auto\">\n            <div className=\"bg-white p-6 rounded-2xl shadow-lg border border-theme-gray-100\">\n              <Users className=\"h-8 w-8 text-theme-primary-500 mb-4\" />\n              <h3 className=\"font-semibold text-theme-gray-900 mb-2\">10,000+ Users</h3>\n              <p className=\"text-theme-gray-600 text-sm\">Trusted by professionals worldwide</p>\n            </div>\n            <div className=\"bg-white p-6 rounded-2xl shadow-lg border border-theme-gray-100\">\n              <Zap className=\"h-8 w-8 text-theme-primary-500 mb-4\" />\n              <h3 className=\"font-semibold text-theme-gray-900 mb-2\">Lightning Fast</h3>\n              <p className=\"text-theme-gray-600 text-sm\">Built for speed and performance</p>\n            </div>\n            <div className=\"bg-white p-6 rounded-2xl shadow-lg border border-theme-gray-100\">\n              <Star className=\"h-8 w-8 text-theme-primary-500 mb-4\" />\n              <h3 className=\"font-semibold text-theme-gray-900 mb-2\">5-Star Rated</h3>\n              <p className=\"text-theme-gray-600 text-sm\">Loved by our community</p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  )\n}"
    }
  }
}
