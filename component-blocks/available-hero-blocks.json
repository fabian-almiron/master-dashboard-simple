      "template": "import Link from 'next/link'\nimport Image from 'next/image'\nimport { Shield, Award, Users, Calendar, Star } from 'lucide-react'\n\ninterface HeroProps {\n  headline: string\n  description: string\n  primaryCta: { text: string; href: string }\n  secondaryCta?: { text: string; href: string }\n  heroImage: string\n  credentials: string[]\n  testimonialQuote?: string\n  testimonialAuthor?: string\n}\n\nexport default function Hero({ headline, description, primaryCta, secondaryCta, heroImage, credentials, testimonialQuote, testimonialAuthor }: HeroProps) {\n  return (\n    <section className=\"bg-slate-50 py-20\">\n      <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-12 items-center\">\n          <div>\n            <div className=\"flex items-center mb-4\">\n              <Shield className=\"h-6 w-6 text-blue-600 mr-2\" />\n              <span className=\"text-blue-600 font-semibold text-sm\">Licensed & Trusted Professionals</span>\n            </div>\n            <h1 className=\"text-4xl sm:text-5xl font-bold text-gray-900 mb-6\">\n              {headline}\n            </h1>\n            <p className=\"text-xl text-gray-600 mb-6\">\n              {description}\n            </p>\n            \n            {/* Credentials */}\n            <div className=\"mb-6\">\n              <div className=\"flex items-center mb-3\">\n                <Award className=\"h-5 w-5 text-blue-600 mr-2\" />\n                <span className=\"text-sm font-semibold text-gray-900\">Credentials & Recognition</span>\n              </div>\n              <div className=\"flex flex-wrap gap-2\">\n                {credentials.map((credential, index) => (\n                  <span key={index} className=\"bg-blue-100 text-blue-800 text-xs px-3 py-1 rounded-full\">\n                    {credential}\n                  </span>\n                ))}\n              </div>\n            </div>\n            \n            {/* Testimonial */}\n            {testimonialQuote && (\n              <div className=\"bg-white p-4 rounded-lg border-l-4 border-blue-600 mb-6\">\n                <div className=\"flex items-center mb-2\">\n                  {[...Array(5)].map((_, i) => (\n                    <Star key={i} className=\"h-4 w-4 text-yellow-400 fill-current\" />\n                  ))}\n                </div>\n                <p className=\"text-gray-700 italic text-sm mb-2\">\"{testimonialQuote}\"</p>\n                <p className=\"text-gray-600 text-xs\">- {testimonialAuthor}</p>\n              </div>\n            )}\n            \n            <div className=\"flex flex-col sm:flex-row gap-4\">\n              <Link\n                href={primaryCta.href}\n                className=\"bg-blue-600 text-white px-8 py-3 rounded-lg text-lg font-medium hover:bg-blue-700 transition-colors inline-flex items-center justify-center\"\n              >\n                <Calendar className=\"mr-2 h-5 w-5\" />\n                {primaryCta.text}\n              </Link>\n              {secondaryCta && (\n                <Link\n                  href={secondaryCta.href}\n                  className=\"border border-gray-300 text-gray-700 px-8 py-3 rounded-lg text-lg font-medium hover:bg-gray-50 transition-colors inline-flex items-center justify-center\"\n                >\n                  <Users className=\"mr-2 h-5 w-5\" />\n                  {secondaryCta.text}\n                </Link>\n              )}\n            </div>\n          </div>\n          <div className=\"relative\">\n            <Image\n              src={heroImage}\n              alt=\"Professional team\"\n              width={600}\n              height={400}\n              className=\"rounded-lg shadow-xl\"\n            />\n            <div className=\"absolute -bottom-4 -left-4 bg-white p-4 rounded-lg shadow-lg\">\n              <div className=\"flex items-center\">\n                <div className=\"bg-green-100 p-2 rounded-full mr-3\">\n                  <Shield className=\"h-6 w-6 text-green-600\" />\n                </div>\n                <div>\n                  <p className=\"text-sm font-semibold text-gray-900\">Fully Licensed</p>\n                  <p className=\"text-xs text-gray-600\">& Insured</p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  )\n}"
    },
    {
      "id": "hero-healthcare-medical",
      "name": "Healthcare & Medical Hero",
      "description": "Compassionate hero for healthcare services with appointment booking and patient focus",
      "tags": ["healthcare", "medical", "compassionate", "appointments", "patient-care", "trust"],
      "style": "medical-compassionate",
      "complexity": "medium",
      "features": ["patient-focus", "appointment-booking", "emergency-info", "care-team", "medical-image"],
      "suitable_for": ["healthcare", "medical", "dental", "clinic", "hospital", "therapy"],
      "template": "import Link from 'next/link'\nimport Image from 'next/image'\nimport { Heart, Calendar, Phone, Clock, Users, Shield } from 'lucide-react'\n\ninterface HeroProps {\n  headline: string\n  description: string\n  primaryCta: { text: string; href: string }\n  secondaryCta?: { text: string; href: string }\n  heroImage: string\n  emergencyPhone: string\n  officeHours: string\n  patientStats?: { yearsExperience: string; patientsServed: string; satisfactionRate: string }\n}\n\nexport default function Hero({ headline, description, primaryCta, secondaryCta, heroImage, emergencyPhone, officeHours, patientStats }: HeroProps) {\n  return (\n    <section className=\"bg-blue-50 py-20\">\n      <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-12 items-center\">\n          <div>\n            <div className=\"flex items-center mb-4\">\n              <Heart className=\"h-6 w-6 text-blue-600 mr-2\" />\n              <span className=\"text-blue-600 font-semibold text-sm\">Compassionate Healthcare</span>\n            </div>\n            <h1 className=\"text-4xl sm:text-5xl font-bold text-gray-900 mb-6\">\n              {headline}\n            </h1>\n            <p className=\"text-xl text-gray-600 mb-6\">\n              {description}\n            </p>\n            \n            {/* Emergency & Hours */}\n            <div className=\"grid grid-cols-1 sm:grid-cols-2 gap-4 mb-6\">\n              <div className=\"bg-red-50 border border-red-200 p-4 rounded-lg\">\n                <div className=\"flex items-center mb-2\">\n                  <Phone className=\"h-5 w-5 text-red-600 mr-2\" />\n                  <span className=\"text-sm font-semibold text-red-800\">Emergency</span>\n                </div>\n                <p className=\"text-red-700 font-medium\">{emergencyPhone}</p>\n                <p className=\"text-red-600 text-xs\">Available 24/7</p>\n              </div>\n              <div className=\"bg-blue-50 border border-blue-200 p-4 rounded-lg\">\n                <div className=\"flex items-center mb-2\">\n                  <Clock className=\"h-5 w-5 text-blue-600 mr-2\" />\n                  <span className=\"text-sm font-semibold text-blue-800\">Office Hours</span>\n                </div>\n                <p className=\"text-blue-700 text-sm\">{officeHours}</p>\n              </div>\n            </div>\n            \n            {/* Patient Stats */}\n            {patientStats && (\n              <div className=\"grid grid-cols-3 gap-4 mb-6\">\n                <div className=\"text-center\">\n                  <p className=\"text-2xl font-bold text-blue-600\">{patientStats.yearsExperience}</p>\n                  <p className=\"text-xs text-gray-600\">Years Experience</p>\n                </div>\n                <div className=\"text-center\">\n                  <p className=\"text-2xl font-bold text-blue-600\">{patientStats.patientsServed}</p>\n                  <p className=\"text-xs text-gray-600\">Patients Served</p>\n                </div>\n                <div className=\"text-center\">\n                  <p className=\"text-2xl font-bold text-blue-600\">{patientStats.satisfactionRate}</p>\n                  <p className=\"text-xs text-gray-600\">Satisfaction Rate</p>\n                </div>\n              </div>\n            )}\n            \n            <div className=\"flex flex-col sm:flex-row gap-4\">\n              <Link\n                href={primaryCta.href}\n                className=\"bg-blue-600 text-white px-8 py-3 rounded-lg text-lg font-medium hover:bg-blue-700 transition-colors inline-flex items-center justify-center\"\n              >\n                <Calendar className=\"mr-2 h-5 w-5\" />\n                {primaryCta.text}\n              </Link>\n              {secondaryCta && (\n                <Link\n                  href={secondaryCta.href}\n                  className=\"border border-blue-300 text-blue-700 px-8 py-3 rounded-lg text-lg font-medium hover:bg-blue-50 transition-colors inline-flex items-center justify-center\"\n                >\n                  <Users className=\"mr-2 h-5 w-5\" />\n                  {secondaryCta.text}\n                </Link>\n              )}\n            </div>\n          </div>\n          <div className=\"relative\">\n            <Image\n              src={heroImage}\n              alt=\"Healthcare professionals\"\n              width={600}\n              height={400}\n              className=\"rounded-lg shadow-xl\"\n            />\n            <div className=\"absolute -bottom-4 -right-4 bg-white p-4 rounded-lg shadow-lg\">\n              <div className=\"flex items-center\">\n                <div className=\"bg-green-100 p-2 rounded-full mr-3\">\n                  <Shield className=\"h-6 w-6 text-green-600\" />\n                </div>\n                <div>\n                  <p className=\"text-sm font-semibold text-gray-900\">HIPAA Compliant</p>\n                  <p className=\"text-xs text-gray-600\">Your privacy protected</p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  )\n}"
    },
