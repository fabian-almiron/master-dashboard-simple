      "template": "import Link from 'next/link'\nimport Image from 'next/image'\nimport { Calendar, Clock, Star, Utensils, MapPin, Gift } from 'lucide-react'\n\ninterface HeroProps {\n  headline: string\n  description: string\n  primaryCta: { text: string; href: string }\n  secondaryCta?: { text: string; href: string }\n  heroImage: string\n  specialOffer?: string\n  hours: string\n  location: string\n  menuHighlights: string[]\n}\n\nexport default function Hero({ headline, description, primaryCta, secondaryCta, heroImage, specialOffer, hours, location, menuHighlights }: HeroProps) {\n  return (\n    <section className=\"bg-amber-50 py-20\">\n      <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-12 items-center\">\n          <div>\n            <div className=\"flex items-center mb-4\">\n              <Utensils className=\"h-6 w-6 text-amber-600 mr-2\" />\n              <span className=\"text-amber-600 font-semibold text-sm\">Fresh • Local • Delicious</span>\n            </div>\n            <h1 className=\"text-4xl sm:text-5xl font-bold text-gray-900 mb-6\">\n              {headline}\n            </h1>\n            <p className=\"text-xl text-gray-600 mb-6\">\n              {description}\n            </p>\n            \n            {/* Special Offer */}\n            {specialOffer && (\n              <div className=\"bg-amber-100 border border-amber-200 p-4 rounded-lg mb-6\">\n                <div className=\"flex items-center mb-2\">\n                  <Gift className=\"h-5 w-5 text-amber-600 mr-2\" />\n                  <span className=\"text-sm font-semibold text-amber-800\">Special Offer</span>\n                </div>\n                <p className=\"text-amber-700 font-medium\">{specialOffer}</p>\n              </div>\n            )}\n            \n            {/* Menu Highlights */}\n            <div className=\"mb-6\">\n              <h3 className=\"text-lg font-semibold text-gray-900 mb-3\">Featured Dishes</h3>\n              <div className=\"grid grid-cols-1 sm:grid-cols-2 gap-2\">\n                {menuHighlights.map((item, index) => (\n                  <div key={index} className=\"flex items-center\">\n                    <Star className=\"h-4 w-4 text-amber-500 mr-2\" />\n                    <span className=\"text-gray-700 text-sm\">{item}</span>\n                  </div>\n                ))}\n              </div>\n            </div>\n            \n            {/* Hours & Location */}\n            <div className=\"grid grid-cols-1 sm:grid-cols-2 gap-4 mb-6\">\n              <div className=\"flex items-center\">\n                <Clock className=\"h-5 w-5 text-amber-600 mr-2\" />\n                <div>\n                  <p className=\"text-sm font-semibold text-gray-900\">Hours</p>\n                  <p className=\"text-sm text-gray-600\">{hours}</p>\n                </div>\n              </div>\n              <div className=\"flex items-center\">\n                <MapPin className=\"h-5 w-5 text-amber-600 mr-2\" />\n                <div>\n                  <p className=\"text-sm font-semibold text-gray-900\">Location</p>\n                  <p className=\"text-sm text-gray-600\">{location}</p>\n                </div>\n              </div>\n            </div>\n            \n            <div className=\"flex flex-col sm:flex-row gap-4\">\n              <Link\n                href={primaryCta.href}\n                className=\"bg-amber-600 text-white px-8 py-3 rounded-lg text-lg font-medium hover:bg-amber-700 transition-colors inline-flex items-center justify-center\"\n              >\n                <Calendar className=\"mr-2 h-5 w-5\" />\n                {primaryCta.text}\n              </Link>\n              {secondaryCta && (\n                <Link\n                  href={secondaryCta.href}\n                  className=\"border border-amber-300 text-amber-700 px-8 py-3 rounded-lg text-lg font-medium hover:bg-amber-50 transition-colors inline-flex items-center justify-center\"\n                >\n                  <Utensils className=\"mr-2 h-5 w-5\" />\n                  {secondaryCta.text}\n                </Link>\n              )}\n            </div>\n          </div>\n          <div className=\"relative\">\n            <Image\n              src={heroImage}\n              alt=\"Restaurant ambiance\"\n              width={600}\n              height={400}\n              className=\"rounded-lg shadow-xl\"\n            />\n            <div className=\"absolute -top-4 -left-4 bg-white p-3 rounded-lg shadow-lg\">\n              <div className=\"flex items-center\">\n                <div className=\"flex\">\n                  {[...Array(5)].map((_, i) => (\n                    <Star key={i} className=\"h-4 w-4 text-yellow-400 fill-current\" />\n                  ))}\n                </div>\n                <span className=\"ml-2 text-sm font-semibold text-gray-900\">4.9</span>\n              </div>\n              <p className=\"text-xs text-gray-600 mt-1\">500+ Reviews</p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </section>\n  )\n}"
    },
