      "suitable_for": ["business", "agency", "portfolio", "product"],
      "template": "import Link from 'next/link'\nimport Image from 'next/image'\nimport { Check } from 'lucide-react'\n\ninterface HeroProps {\n  headline: string\n  description: string\n  features: string[]\n  ctaText: string\n  ctaHref: string\n  heroImage: string\n}\n\nexport default function Hero({ headline, description, features, ctaText, ctaHref, heroImage }: HeroProps) {\n  return (\n    <section className=\"bg-white py-20\">\n      <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div className=\"grid grid-cols-1 lg:grid-cols-2 gap-12 items-center\">\n          <div>\n            <h1 className=\"text-4xl sm:text-5xl font-bold text-gray-900 mb-6\">\n              {headline}\n            </h1>\n            <p className=\"text-xl text-gray-600 mb-8\">\n              {description}\n            </p>\n            <ul className=\"space-y-3 mb-8\">\n              {features.map((feature, index) => (\n                <li key={index} className=\"flex items-center\">\n                  <Check className=\"h-5 w-5 text-green-500 mr-3\" />\n                  <span className=\"text-gray-700\">{feature}</span>\n                </li>\n              ))}\n            </ul>\n            <Link\n              href={ctaHref}\n              className=\"bg-blue-600 text-white px-8 py-3 rounded-lg text-lg font-medium hover:bg-blue-700 transition-colors inline-block\"\n            >\n              {ctaText}\n            </Link>\n          </div>\n          <div className=\"relative\">\n            <Image\n              src={heroImage}\n              alt=\"Hero image\"\n              width={600}\n              height={400}\n              className=\"rounded-lg shadow-xl\"\n            />\n          </div>\n        </div>\n      </div>\n    </section>\n  )\n}"
    },
